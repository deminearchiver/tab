{
  "private": true,
  "name": "tab",
  "version": "0.0.0",
  "description": "A modern new tab browser extension",
  "author": "deminearchiver",
  "repository": "https://github.com/deminearchiver/tab",
  "packageManager": "yarn@4.0.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:extension": "turbo run dev --filter=@tab/extension...",
    "dev:website": "turbo run dev --filter=@tab/website...",
    "build": "turbo run build",
    "build:extension": "turbo run build --filter=@tab/extension...",
    "build:website": "turbo run build --filter=@tab/website..."
  },
  "devDependencies": {
    "turbo": "^1.11.3"
  }
}
